{
  "name": "amber-price-monitor",
  "version": "1.0.0",
  "description": "Monitor Amber Electric price fluctuations and send notifications for high feed-in prices",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && npm run build:web",
    "build:web": "cd web && npm run build",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:web": "cd web && npm run dev",
    "generate-types": "tsx scripts/generate-types.ts"
  },
  "keywords": [
    "amber",
    "electricity",
    "solar",
    "price-monitoring"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "node-cron": "^3.0.3",
    "node-notifier": "^10.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "redis": "^5.10.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/node-notifier": "^8.0.5",
    "@types/ws": "^8.5.13",
    "@vercel/node": "^3.2.21",
    "pino-pretty": "^11.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
